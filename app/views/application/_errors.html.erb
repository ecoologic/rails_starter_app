<%#= render 'errors', model: page %>
<div class="errors errors__explanation">
  <h2>
    <%= pluralize(model.errors.count, "error") %>
    prohibited this
    <%= model.class.to_s.underscore.downcase.humanize %>
    from being saved:
  </h2>

  <ul>
    <% model.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
</div>
